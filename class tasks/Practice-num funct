use DDL;
select abs(-173) as "Absolute Value"
select CEILING(0329.90) as "Celing Value"
select ACOS(0.25) as "Degrees to Radians"
select count(DEPARTMENT_ID) from DEPARTMENTS;
select cos(90) as "Cos Value"
select floor(25.75) as "Floor Value"
select log(99) as "Lorigithm"
select PI() as "PI Value";
select POWER(8,8) as "Power Value"; -- doubt
select RADIANS(90) as "Radians value"
select round(99.90909,3) as "Round Value";
select sqrt(4) as "Square Root Value";
select SQUARE(9) as "Square Value";
